{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"name": "source1"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "compose1"
				}
			],
			"scriptLines": [
				"parameters{",
				"     Source1 as integer ($parameter1),",
